{
    "name": "centerpoint/reader",
    "description": "This package will help you to build a collection from different database of your project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Sanwarul Islam",
            "email": "muhammad.sanwarul94@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Centerpoint\\Reader\\": "src/",
            "Centerpoint\\Reader\\Repository\\": "src/Repositories/"
        }
    },
    "scripts": {
        "post-update-cmd": "Centerpoint\\Reader::postUpdate",
        "post-package-install": [
            "Centerpoint\\Reader::postPackageInstall"
        ],
        "post-install-cmd": [
            "Centerpoint\\Reader::warmCache",
            "phpunit -c app/"
        ],
        "post-autoload-dump": [
            "Centerpoint\\Reader::postAutoloadDump"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Centerpoint\\Reader\\ReaderServiceProvider"
            ]
        }
    }
}
